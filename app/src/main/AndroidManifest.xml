<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.maulik.sign_in">

    <application
        android:allowBackup="true"
        android:icon="@drawable/tcnjcs"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Sign_In"
            android:label="@string/app_name"
            android:windowSoftInputMode="stateHidden">
        </activity>
        <activity android:name=".VisitorList"
            android:label="VisitorList"/>
        <activity android:name=".HomePage"
            android:label="Sign In">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".FacultyHomePage"
            android:label="FacultyHomePage">

        </activity>
    </application>

</manifest>
